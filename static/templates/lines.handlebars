<ul class="lines">
	<li class="trolleys">
		<strong>Тролеи:</strong>
		<div class="lines-wrapper">
			{{#each trolleys}}
				<a class="trolley" href="#/trolley/{{this}}" data-line-name="{{this}}">{{this}}</a>
			{{/each}}
		</div>
	</li>
	<li class="trams">
		<strong>Трамваи:</strong>
		<div class="lines-wrapper">
			{{#each trams}}
				<a class="tram" href="#/tram/{{this}}" data-line-name="{{this}}">{{this}}</a>
			{{/each}}
		</div>
	</li>
	<li class="buses">
		<strong>Автобуси:</strong>
		<div class="lines-wrapper">
			{{#each buses}}
				<a class="bus" href="#/bus/{{this}}" data-line-name="{{this}}">{{this}}</a>
			{{/each}}
		</div>
	</li>
	<li class="subway">
		<div class="lines-wrapper">
			<strong>
				<a class="subway">Метро</a>
			</strong>
		</div>
	</li>
</ul>
