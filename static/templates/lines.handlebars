<ul class="lines">
	<li class="trams">
		<strong>Трамваи:</strong>
		<div class="lines-wrapper">
			{{#each lines.trams}}
				<a class="tram" href="#{{../stopcode}}/{{this.[0]}}" data-line-name="{{this.[0]}}">{{this.[1]}}</a>
			{{/each}}
		</div>
	</li>
	<li class="buses">
		<strong>Автобуси:</strong>
		<div class="lines-wrapper">
			{{#each lines.buses}}
				<a class="bus" href="#{{../stopcode}}/{{this.[0]}}" data-line-name="{{this.[0]}}">{{this.[1]}}</a>
			{{/each}}
		</div>
	</li>
	<li class="trolleys">
		<strong>Тролеи:</strong>
		<div class="lines-wrapper">
			{{#each lines.trolleys}}
				<a class="trolley" href="#{{../stopcode}}/{{this.[0]}}" data-line-name="{{this.[0]}}">{{this.[1]}}</a>
			{{/each}}
		</div>
	</li>
	<li class="subway">
		<div class="lines-wrapper">
			<strong>
				<a class="subway" data-line-type="3">Метро</a>
			</strong>
		</div>
	</li>
</ul>
